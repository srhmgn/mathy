{
  "extends": ["airbnb"],
  "env": {
    "jest/globals": true,
    "browser": true
  },
  "plugins": ["jest", "react-hooks"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-array-index-key": [0],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_" }],
    "react/prop-types": [0]
  }
}